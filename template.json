          "non_ip/microsoft_cdn",
          "domainset/game-download",
          "non_ip/domestic",
          "non_ip/direct",
          "domainset/my_direct"
        ],
        "server": "dns-local"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns-fakeip",
        "rewrite_ttl": 1
      }
    ],
    "final": "dns-pure",
